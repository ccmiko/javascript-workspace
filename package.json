    "run:s3:put": "yarn node:dev ./aws_s3/put.ts"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.171.0",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.171.0",
    "@aws-sdk/s3-request-presigner": "^3.173.0",
    "dotenv": "^16.0.2",
    "uuid": "^9.0.0"
  }
}
